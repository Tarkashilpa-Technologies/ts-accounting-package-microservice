    "IF_PAY_GST": {
      "type": "boolean"
    },
    "IGST": {
      "type": "decimal"
    },
    "CGST": {
      "type": "decimal"
    },
    "SGST": {
      "type": "decimal"
    },
    "GRAND_TOTAL": {
      "type": "decimal"
    },
    "CLIENT_DEDUCT_GST": {
      "type": "boolean"
